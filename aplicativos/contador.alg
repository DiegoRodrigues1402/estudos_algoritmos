algoritmo "semnome"
// Função :
// Autor :
// Data : 09/05/2021
// Seção de Declarações 
var
    contador ,cont ,salt: inteiro
inicio
escreva ("escreva numero ")
leia(cont)
escreva("de quantosem quantos")
leia(salt)
     contador <- 0
     Enquanto (contador <= cont) faca
       Escreval(contador)

     contador <- contador + salt

     fimEnquanto

fimalgoritmo

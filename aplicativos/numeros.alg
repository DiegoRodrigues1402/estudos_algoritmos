algoritmo "semnome"
// Função :
// Autor :
// Data : 14/05/2021
// Seção de Declarações 
var
    t, c ,n , m:inteiro
inicio
n <- 0
 escreva(n)
 m <- 1
   escreva(m)

c <- 3
enquanto (C <= 15) faca
           t <- m + n
             escreva(t)
             n<- m
              m <- t

c <- c + 1
fimenquanto
fimalgoritmo

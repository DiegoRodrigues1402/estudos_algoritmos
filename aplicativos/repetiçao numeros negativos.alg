algoritmo "semnome"
// Função :
// Autor :
// Data : 10/05/2021
// Seção de Declarações 
var
    cont , num ,res:inteiro

inicio
cont <- 0
num <- 0
repita
escreval("digite um numero")
   leia (res)
   se(res < 0) entao
   num <- num + 1
   fimse
   
cont <-cont +1
ate(cont > 5)
escreva(num)
fimalgoritmo

algoritmo "semnome"
// Função :
// Autor :
// Data : 10/05/2021
// Seção de Declarações 
var
    n,s:inteiro
   res:caractere
inicio


  repita
escreva(  "digite um numero  ")
leia(n)
s <- s + n
         escreva(" voce que continuar? [s/n]")
      leia (res)
  ate (res = "n")

escreva(" a soma total é  ", s)
fimalgoritmo

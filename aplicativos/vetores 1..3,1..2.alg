algoritmo "semnome"
// Função :
// Autor :
// Data : 20/05/2021
// Seção de Declarações 
var
mat:vetor[1..3,1..2] de inteiro
l,c:inteiro
inicio

         para l<- 1 ate 3 faca
         para  c<- 1 ate 2 faca
         escreva("digite um valor de [",l," e",c,"]" )
         leia(mat[l,c])
         fimpara
         fimpara
         para l<- 1 ate 3 faca
         para c<- 1 ate 2 faca
         escreva(mat[l,c]:5)
         fimpara
          escreval()
         fimpara


fimalgoritmo

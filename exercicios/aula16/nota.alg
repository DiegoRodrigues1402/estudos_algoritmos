algoritmo "semnome"
// Função :
// Autor :
// Data : 04/05/2021
// Seção de Declarações 
var
     n1 ,n2 ,soma:real
inicio
 escreval("digite sua nota")
 leia(n1)
 escreval("digite outra nota")
 leia (n2)
 soma <- n1+n2
 escreval ("sua nota é ", soma)
 se (soma >= 7)entao
 escreval ("voce foi aprovado")
 senao
 escreval("reprovado")
 fimSe
fimalgoritmo
